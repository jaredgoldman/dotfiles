    "when": "!terminalFocus"
  },
  {
    "key": "space f c",
    "command": "workbench.action.findInFiles",
    "when": "!terminalFocus",
    "args": {
      "query": "",
      "triggerSearch": false,
      "focusResults": false,
      "isRegex": false,
      "isCaseSensitive": false,
      "matchWholeWord": false
    }
  },
  {
    "key": "space t h",
